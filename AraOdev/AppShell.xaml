<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="AraOdev.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:AraOdev"
    Shell.FlyoutBehavior="Flyout"
    Title="AraOdev">

    <ShellContent
        Title="Ana sayfası"
        Icon="dotnet_bot.png"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Kredı Hesaplama"
    
        Icon="kredi.jfif"
        ContentTemplate="{DataTemplate local:Kredı}"
        Route="Kredı" />
    <ShellContent
         Title="Vücüt kitle indexsi"
         Icon="kilo.jfif"
         ContentTemplate="{DataTemplate local:NewPage1}"
         Route="NewPage1" />
    <ShellContent
         Title="Renk Seçım"
        Icon="renk.jfif"
         ContentTemplate="{DataTemplate local:Renk_Secım}"
         Route="Renk_Secım" />
    <ShellContent
         Title="Ypılacaklar listesi"
          Icon="todolist.png"
         ContentTemplate="{DataTemplate local:Yapılacaklar}"
         Route="Yapılacaklar" />


</Shell>
